{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "trailing-comma": false,
        "variable-name": [true, "ban-keywords", "allow-leading-underscore"],
        "no-console": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}